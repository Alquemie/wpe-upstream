{
    "name": "alquemie/wpe-upstream",
    "type": "project",
    "version": "24.03.14.2",
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        {
          "type": "composer",
          "url": "https://alquemie.github.io"
        },
        {
          "type": "vcs",
          "url": "https://github.com/roots/wp-password-bcrypt.git"
        }
    ],
    "require": {
        "alquemie/advanced-custom-fields-pro": "^6.0",
        "alquemie/alquemie-config": "^0.2",
        "alquemie/gravityforms": "^2.0",
        "jenwachter/php-rsync": "*",
        "roots/wp-password-bcrypt": "^1.0.0",
        "wpackagist-plugin/easy-wp-smtp":"^2.2",
        "wpackagist-plugin/wordpress-importer": "^0.8"
    },
    "require-dev": {

    },
    "config": {
      "vendor-dir": "vendor",
      "preferred-install": "dist",
      "optimize-autoloader": true,
      "sort-packages": true,
      "platform": {
        "php": "8.2"
      },
      "allow-plugins": {
        "composer/installers": true,
        "johnpbloch/wordpress-core-installer": true
      }
    }
}
